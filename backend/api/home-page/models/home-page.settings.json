{
  "kind": "singleType",
  "connection": "default",
  "collectionName": "home_pages",
  "info": {
    "name": "Home Page",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "about": {
      "type": "richtext"
    },
    "newProductDays": {
      "type": "integer",
      "default": 7,
      "private": true,
      "min": 1
    },
    "information": {
      "type": "text"
    },
    "informationSeverity": {
      "type": "enumeration",
      "enum": [
        "success",
        "info",
        "warning",
        "error"
      ],
      "default": "info"
    }
  }
}
